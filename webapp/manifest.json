{
	"_version": "1.12.0",
	"sap.app": {
		"id": "mrg.strada",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"northwind": {
				"type": "OData",
				"uri": "V2/Northwind/Northwind.svc",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "mrg.strada.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.140.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"handleValidation": true,
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "mrg.strada.i18n.i18n"
				}
			},
			"Northwind": {
				"dataSource": "northwind"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"bypassed": {
					"target": "NotFound"
				},
				"viewType": "XML",
				"viewPath": "mrg.strada.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
				{
					"name": "RouteMain",
					"pattern": ":?query:",
					"target": [
						"TargetMain"
					]
				},
				{
					"name": "Detail",
					"pattern": "detailView/{path}",
					"target": [
						"Detail"
					]
				},
				{
					"name": "NotFound",
					"pattern": "notFound",
					"target": [
						"NotFound"
					]
				}
			],
			"targets": {
				"TargetMain": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "main",
					"viewName": "Main"
				},
				"Detail": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "detail",
					"viewName": "Details"
				},
				"NotFound": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "notfound",
					"viewName": "NotFound"
				}
			}
		}
	}
}